{
  "name": "tik",
  "version": "1.0.0",
  "type": "module",
  "main": "server.mjs",
  "bin": "server.mjs",
  "scripts": {
    "start": "node server.mjs",
    "build:win": "pkg . --targets node20-win-x64 --output dist/tik-win.exe",
    "build:linux": "pkg . --targets node20-linux-x64 --output dist/tik-linux",
    "build:all": "pkg . --targets node20-linux-x64,node20-win-x64 --output dist/tik"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "TikTok Downloader",
  "dependencies": {
    "@tobyg74/tiktok-api-dl": "^1.3.2",
    "node-fetch": "^3.3.2",
    "open": "^10.1.2"
  },
  "pkg": {
    "assets": [
      "index.html"
    ],
    "scripts": [
      "server.mjs"
    ],
    "outputPath": "dist"
  }
}

